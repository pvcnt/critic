    "@react-router/node": "^7.1.5",
    "@react-router/serve": "^7.1.5",
    "clsx": "^2.1.1",
    "isbot": "^5.1.17",
    "javascript-time-ago": "^2.5.11",
    "normalize.css": "^8.0.1",
