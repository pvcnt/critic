{
  "name": "web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "serve": "dotenv -e .env.local -- react-router-serve ./build/server/*/index.js",
    "typecheck": "pnpm run codegen && tsc -b",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "codegen": "pnpm run prisma:generate && react-router typegen",
    "prisma:generate": "dotenv -e .env.local -- prisma generate",
    "prisma:validate": "dotenv -e .env.local -- prisma validate",
    "prisma:push": "dotenv -e .env.local -- prisma db push"
  },
  "dependencies": {
