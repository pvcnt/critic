{
  "name": "web",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "preview": "pnpm run build && vite preview",
    "typecheck": "react-router typegen && tsc -b",
    "test": "vitest run",
